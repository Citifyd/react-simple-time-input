{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "extends": ["standard", "standard-react"],
  "rules": {
    "no-multiple-empty-lines": ["error", { "max": 2 }],
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "no-console": 1
  }
}